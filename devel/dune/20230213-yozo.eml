+CONFIGURE_ARGS =	--bindir ${TRUEPREFIX}/bin \
+			--sbindir ${TRUEPREFIX}/sbin \
+			--datadir ${TRUEPREFIX}/share \
+			--etcdir ${SYSCONFDIR} \
+			--libdir ${TRUEPREFIX}/lib/ocaml \
