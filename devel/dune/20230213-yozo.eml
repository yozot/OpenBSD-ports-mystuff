--- /usr/ports/devel/dune/Makefile	Sat Mar 12 11:03:51 2022
+++ ./Makefile	Mon Feb 13 08:48:51 2023
@@ -1,19 +1,15 @@
-MULTI_PACKAGES =	-main -private -configurator
